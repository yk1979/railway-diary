{
  "name": "railway-diary",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:yk1979/railway-diary.git"
  },
  "scripts": {
    "dev": "nodemon",
    "agreed": "agreed-server --path spec/agreed/index.ts --port 4010",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "run-s lint:*",
    "lint:js": "eslint --ext .ts,.tsx src --cache",
    "lint:css": "stylelint 'src/**/*.tsx'",
    "fix": "eslint --ext .ts,.tsx src  --fix --cache",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts, tsx}": "npm run fix"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "license": "ISC",
  "dependencies": {
    "@specter/client": "0.6.5",
    "@specter/specter": "0.6.5",
    "@zeit/next-typescript": "1.1.1",
    "date-fns": "2.16.1",
    "date-fns-tz": "1.0.12",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "firebase": "8.0.1",
    "firebase-admin": "9.3.0",
    "next": "10.0.1",
    "now": "20.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "3.11.0",
    "react-modal": "3.11.2",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-logger": "3.0.6",
    "redux-saga": "1.1.3",
    "session-file-store": "1.5.0",
    "styled-components": "5.2.1",
    "styled-reset": "4.3.0",
    "typescript-fsa": "3.0.0",
    "typescript-fsa-reducers": "1.2.2",
    "uuid": "8.3.1"
  },
  "devDependencies": {
    "@agreed/cli": "5.1.10",
    "@agreed/typed": "5.1.10",
    "@babel/core": "7.12.3",
    "@storybook/addon-actions": "6.0.28",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-knobs": "6.0.28",
    "@storybook/addon-links": "6.0.28",
    "@storybook/addon-viewport": "6.0.28",
    "@storybook/addons": "6.0.28",
    "@storybook/react": "6.0.28",
    "@types/express": "4.17.9",
    "@types/express-session": "1.17.0",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/react-modal": "3.10.6",
    "@types/react-redux": "7.1.11",
    "@types/redux-logger": "3.0.8",
    "@types/session-file-store": "1.2.1",
    "@types/styled-components": "5.1.4",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "1.11.1",
    "babel-preset-react-app": "10.0.0",
    "cross-env": "7.0.2",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
